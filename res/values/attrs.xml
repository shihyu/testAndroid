<?xml version="1.0" encoding="utf-8"?>
<resources >
	<attr name="text_color" format="color"/>   
	<attr name="text_size" format="dimension"/>
	<attr name="text_offset" format="dimension"/>
	<attr name="reach_color" format="color"></attr>
	<attr name="reach_height" format="dimension"></attr>
	<attr name="unreach_color" format="color"></attr>
	<attr name="unreach_height" format="dimension"></attr>
	
	<declare-styleable name="HorizontalProgressBar" >
		<attr name="text_color" />   
		<attr name="text_size" />
		<attr name="text_offset" />
		<attr name="reach_color" ></attr>
		<attr name="reach_height" ></attr>
		<attr name="unreach_color" ></attr>
		<attr name="unreach_height" ></attr>
	</declare-styleable>
	
	<declare-styleable name="CircleProgressBar" >
		<attr name="radius" format="dimension" />
	</declare-styleable>


	<!--刮刮卡-->
	<attr name="text" format="string"/>
	<attr name="textColor" format="color"/>
	<attr name="textSize" format="dimension"/>

	<declare-styleable name="GuaView">
		<attr name="text" />
		<attr name="textColor" />
		<attr name="textSize" />
	</declare-styleable>
</resources>
